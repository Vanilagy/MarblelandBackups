// Easter egg key
$EEKeys = 0; // Number of keys in the level
$EEKeysCollected = 0; // Number of keys collected
function doTheEEThing() {
	MissionGroup.add(
		new InteriorInstance() {
			position = "-2.1 5.4 0.5";
			rotation = "1 0 0 0";
			scale = "1 1 1";
			interiorFile = "~/data/interiors/WeatherWonders/Nest.dif";
			showTerrainInside = "0";
		}
	);
	MissionGroup.add(
		new Item() {
			position = "-2.357802 5.198971 -0.3322314";
			rotation = "0.3662673 -0.3882101 -0.8456602 26.51627";
			scale = "1 1 1";
			dataBlock = "easterEgg";
			collideable = "0";
			static = "1";
			rotate = "0";
		}
	);
	messageClient(localClientConnection.player.client, "", "\c0Nice! Go to the hub for your prize.");
}
// Debug
function resetKeys() {
	$EEKeys = 0;
	withAll(EasterEggKey, "$EEKeys++; %this.hide(false); %this.startFade(0, 0, false); %this.collected = false;");
	$EEKeysCollected = 0;
}
package EEKeys {
	function toggleEditor(%make) {
		Parent::toggleEditor(%make);
		resetKeys();
	}
};
activatePackage(EEKeys);

datablock ItemData(EasterEggKey) {
	category = "Powerups";
	className = "PowerUp";
	pickupAudio = PuAntiGravityVoiceSfx;
	pickupName = "an easter egg key.";
	noRespawn = true;
	shapeFile = "~/data/shapes/WeatherWonders/EasterEggKey.dts";
	emap = false;
	mass = 1;
	friction = 1;
	elasticity = 0.3;
	maxInventory = 1;
};
function EasterEggKey::onAdd(%this, %obj) {
	%obj.playThread(0, "Ambient");
	%obj.collected = false;
	$EEKeys++;
}
function EasterEggKey::onPickup(%this, %obj, %user, %amount) {
	//Parent::onPickup(%this, %obj, %user, %amount);
	//%obj.hide(true);
	// Don't use hide() so it can fade instead.
	if (!%obj.collected) {
		%obj.collected = true;
		$EEKeysCollected++;
		%obj.startFade(3000, 0, true);
		%obj.playAudio(0, EEKeySFX);
		messageClient(localClientConnection.player.client, "", "\c0You've collected an easter egg key. " @ $EEKeys - $EEKeysCollected @ " to go.");
		if ($EEKeysCollected == $EEKeys) {
			doTheEEThing();
		}
	}
}
function EasterEggKey::onMissionReset(%this, %obj) {
	if (%obj.collected) {
		// Hide it
		//%obj.hide(true);
		%obj.startFade(0, 0, true);
	}
}

