// So I was planning to make these RaftTrigger methods, but TorqueScript.
function checkStartVar(%trigger) {
	// Create a variable in the raft so it can go back to where it was when the level restarts.
	if (%trigger.raft.originalPos $= "") {
		%trigger.raft.originalPos = %trigger.raft.getTransform();
	}
}

function raftReset(%trigger) {
	// Called when you respawn.
	
	checkStartVar(%trigger);
	
	// Snap the raft back to the starting position.
	%trigger.raft.setTransform(%trigger.raft.originalPos);
}

function doubleRaftReset(%trigger) {
	// Either I'm terrible at coding or this game is wierd . . . at least without knowledge of how this game works.
	schedule(RaftTrigger.tickPeriodMS + 10, 0, "raftReset", %trigger);
	schedule(RaftTrigger.tickPeriodMS + 100, 0, "raftReset",  %trigger);
}

function RaftTrigger::onMissionReset(%this, %trigger) {
	doubleRaftReset(%trigger);
}

package RaftTrigger {
	function clientCbOnRespawnOnCheckpoint() {
		withAll(RaftTrigger, "doubleRaftReset(%this);");
		Parent::clientCbOnRespawnOnCheckpoint();
	}
};
activatePackage(RaftTrigger);

// Update the raft.
function SimObject::raftUpdate(%trigger, %serverOnce, %obj) {
	// HiGuy's original line for getting the client-side copy of the object: %clientObj = getClientSyncObject(%obj.getSyncId());
	
	%raftPos = %trigger.raft.getPosition();
	%rX = getWord(%raftPos, 0);
	%rY = getWord(%raftPos, 1);
	%rZ = getWord(%raftPos, 2);
	%raftRot = getWords(%trigger.raft.getTransform(), 3, 6);
	%mPos = VectorAdd(%obj.getPosition(), %trigger.offset); // Marble position + offset.
	%mX = getWord(%mPos, 0);
	%mY = getWord(%mPos, 1);
	%mZ = getWord(%mPos, 2);
	%cRaft = getClientSyncObject(%trigger.raft.getSyncId());
	
	// Here be craziness:
	if (%trigger.axis $= "XY") {
		// Server update
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %mX SPC %mY SPC %rZ SPC %raftRot ); }
		// Client update
		%cRaft.setTransform( %mX SPC %mY SPC %rZ SPC %raftRot );
	}
	else if (%trigger.axis $= "X") {
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %mX SPC %rY SPC %rZ SPC %raftRot ); }
		%cRaft.setTransform( %mX SPC %rY SPC %rZ SPC %raftRot );
	}
	else if (%trigger.axis $= "Y") {
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %rX SPC %mY SPC %rZ SPC %raftRot ); }
		%cRaft.setTransform( %rX SPC %mY SPC %rZ SPC %raftRot );
	}
	else if (%trigger.axis $= "Z") {
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %rX SPC %rY SPC %mZ SPC %raftRot ); }
		%cRaft.setTransform( %rX SPC %rY SPC %mZ SPC %raftRot );
	}
	else if (%trigger.axis $= "XZ") {
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %mX SPC %rY SPC %mZ SPC %raftRot ); }
		%cRaft.setTransform( %mX SPC %rY SPC %mZ SPC %raftRot );
	}
	else if (%trigger.axis $= "YZ") {
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %rX SPC %mY SPC %mZ SPC %raftRot ); }
		%cRaft.setTransform( %rX SPC %mY SPC %mZ SPC %raftRot );
	}
	else { // XYZ
		if (%trigger.updateServer || %serverOnce) { %trigger.raft.setTransform( %mPos SPC %raftRot ); }
		%cRaft.setTransform( %mPos SPC %raftRot );
	}
	
	// Re-run the function
	if (!%serverOnce) { %trigger.raftSchedule = %trigger.schedule(1, "raftUpdate", 0, %obj); }
}

datablock TriggerData(RaftTrigger) {
	tickPeriodMS = 3;
};
function RaftTrigger::onAdd(%this, %trigger) {
	// Name of the raft object.
	if (%trigger.raft $= "") { %trigger.raft = "Name of raft"; }
	// Which axis to allow the raft to move on. Put either the axis letter(s) or anything else for all three.
	if (%trigger.axis $= "") { %trigger.axis = "XY"; }
	// Whether to update the server-side raft too. If something doesn't work, try setting this to 1.
	if (%trigger.updateServer $= "") { %trigger.updateServer = 0; }
	// This is added to any axes of the raft's position that are affected.
	if (%trigger.offset $= "") { %trigger.offset = "0 0 0"; }
	// Optional setting to modify the marble's maxRollVelocity when on the raft. To be used, this needs to be above zero. Default maxRollVelocity is 15.
	if (%trigger.marbleSpeed $= "") { %trigger.marbleSpeed = 0; }
	// Optional setting to disallow jumping on the raft.
	if (%trigger.allowJumping $= "") { %trigger.allowJumping = 1; }
	// Optional setting to play an AudioProfile when the raft is triggered.
	if (%trigger.inAudio $= "") { %trigger.inAudio = "Audio profile"; }
	// Optional setting to play an AudioProfile when the raft is un-triggered.
	if (%trigger.outAudio $= "") { %trigger.outAudio = "Audio profile"; }
}
function RaftTrigger::onEnterTrigger(%this, %trigger, %obj) {
	warn("Entered RaftTrigger \"" @ %trigger.getName() @ "\" (ID: " @ %trigger.getId() @ ")");
	
	checkStartVar(%trigger);
	
	// Modify the physics.
	%marble = %obj.getDataBlock(); // yay for non-hard-coding
	if (%trigger.marbleSpeed > 0) {
		%trigger.defMRV = %marble.maxRollVelocity;
		%marble.maxRollVelocity = %trigger.marbleSpeed;
	}
	if (!%trigger.allowJumping) {
		%trigger.defJI = %marble.jumpImpulse;
		%marble.jumpImpulse = 0;
	}
	
	// Start up the loop
	%trigger.raftUpdate(0, %obj);
	
	// Audio
	if (%trigger.inAudio !$= "") { %obj.playAudio(0, %trigger.inAudio); }
}
function RaftTrigger::onLeaveTrigger(%this, %trigger, %obj) {
	warn("Exited RaftTrigger \"" @ %trigger.getName() @ "\" (ID: " @ %trigger.getId() @ ")");
	
	// Modify the physics.
	%marble = %obj.getDataBlock();
	if (%trigger.marbleSpeed > 0) {
		%marble.maxRollVelocity = %trigger.defMRV;
	}
	if (!%trigger.allowJumping) {
		%marble.jumpImpulse = %trigger.defJI;
	}
	
	// Stop the loop.
	cancel(%trigger.raftSchedule @ %trigger.scheduleName);
	%trigger.raftUpdate(1, %obj);
	
	// Audio
	if (%trigger.outAudio !$= "") { %obj.playAudio(0, %trigger.outAudio); }
	
	// Remove any no longer used variables.
	%trigger.defMRV = "";
	%trigger.defJI = "";
}


