	if (%trigger.text $= "") { %trigger.text = "Text"; }
	if (%trigger.resetOnRespawn $= "") { %trigger.resetOnRespawn = "1"; }
	if (%trigger.minGems $= "") { %trigger.minGems = "0"; }
}
