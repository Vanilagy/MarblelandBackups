//Make sure the rainbow gem exists, otherwise substitute it with a (revalued) black gem
if (!isObject(GemItemRainbow)) {
   eval("datablock ItemData(GemItemRainbow : GemItemBlack) { skin = \"black\"; };");
   $deleteRainbowGem = 1;
}
function serverCbOnMissionLoaded() {
   if ($deleteRainbowGem) {
      GemItemBlack.huntExtraValue = 19;
   }
}
function serverCbOnMissionEnded() {
   if ($deleteRainbowGem) {
      GemItemRainbow.delete();
      GemItemBlack.huntExtraValue = -2;
   }
}

