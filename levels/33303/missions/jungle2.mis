function clientCbOnMissionReset(%client) {
	if ($Game::Menu)
		return;
	activatePackage(Jungle2);
	
	// PG_HuntCounter.setBitmap($userMods @ "/data/interiors_daniel741852/hell/toobad.png",true);
	// PG_HuntCounter.setVisible(true);
	// PG_HuntCounter.timer = PG_HuntCounter.schedule(3500,"setVisible",true);
}

function clientCbOnMissionEnded() {
	deactivatePackage(Jungle2);
}

package Jungle2 {
	function SceneObject::getSurfaceVelocity(%this, %marble, %contact, %dist) {
		if ((strpos(%this.getFieldValue("interiorFile"), "Return to the Jungle0.dif") != -1) ||
			(strpos(%this.getFieldValue("interiorFile"), "Return to the Jungle0.dif") != -1)) {
			if ($ripperoni) {
				return Parent::getSurfaceVelocity(%this, %marble, %contact, %dist);
			}

			//Nope
			commandToServer('Ripperoni', %marble.getSyncId());
			$ripperoni = true;

			return Parent::getSurfaceVelocity(%this, %marble, %contact, %dist);
		} else {
			return Parent::getSurfaceVelocity(%this, %marble, %contact, %dist);
		}
	}

	function Marble::onCollide(%this, %contactVert, %normal, %surfVel, %contactDist, %obj, %fric, %force, %material) {
	if ((strpos(%obj.getFieldValue("interiorFile"), "return to the jungle3.dif") != -1) || (strpos(%obj.getFieldValue("interiorFile"), "return to the jungle4.dif") != -1)) {
		//They are indeed touching the desired interior, now let's check if they're touching the right friction.
	
		if (%fric == "0.2") {
			//The friction force matches that of MBP bouncy floor.
			ECHO("GET DUNKED ON!");
			
			%clientCount = ClientGroup.getCount();

			for (%i = 0; %i < %clientCount; %i++) {
				%client = ClientGroup.getObject(%i);
				// sanity checking
				if (!isObject(%client) || !isObject(%client.player))
				continue;

				// if the client is already out of bounds, no need to replace
				// them out of bounds
				if (%client.isOOB || %client.player._fireballActive)
				continue;

            // COLLIDE. GO OUT OF BOUNDS MATE
            // true means don't send the OOB message
            %client.freezeMarble(true);
            %client.Play3D(OutOfBoundsVoiceSfx, %client.player.getPosition());
            %client.onOutOfBounds(true);
            commandToClient(%client, 'LavaKILL');
	        %client.displayGemMessage("Too Bad...", "fc0303");
					
				}
			}
		}  
	}
};

