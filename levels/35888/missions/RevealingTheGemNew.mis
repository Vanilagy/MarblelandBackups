{	
	%obj.playThread(0, "minute");
	%obj.playThread(1, "hour");

	return TimeTravelItem::onAdd(%this, %obj);
}
function TimeTravelItem5Sec_BM::onPickup(%this,%obj,%user,%amount) {
	return TimeTravelItem::onPickup(%this, %obj, %user, %amount);
}
