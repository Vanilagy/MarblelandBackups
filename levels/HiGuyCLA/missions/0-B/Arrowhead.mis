// Jeff: custom code that allows the user to end the game when picking up the
//       final gem
package endWithTheGems {
   function Gem::onPickup(%this, %obj, %user, %amount) {
      Parent::onPickup(%this, %obj, %user, %amount);
      // Jeff: use missionInfo.endWithGems so that it don't conflict with other levels
      if (%user.client.gemCount >= $Game::GemCount && (MissionInfo.endWithGems || MissionInfo.endWithGems $= "1")) {
         %user.setMode("Victory");
         setGameState("end");
      }
   }
};
activatePackage(endWithTheGems);