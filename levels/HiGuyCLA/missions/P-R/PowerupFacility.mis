package specialHelicopter
{
  function mouseFire(%val)
  {
     if ($BUZZ::foundHelicopter && %val)
     {
        // Jeff: if we have found the helicopter, set the powerup!
        $BUZZ::usingHelicopter = !$BUZZ::usingHelicopter;
        %player = localClientConnection.player;
        if (!$BUZZ::usingHelicopter)
        {
           %player.setPowerUpId(0, true);
           %player.playAudio(0, DoHelicopterSfx);
        }
        else
        {
           %player.setPowerUpId(5, true);
           %player.stopAudio(0);
        }
     }
     Parent::mouseFire(%val);
  }

  function PowerUp::onPickup(%this, %obj, %user, %amount)
  {
     %name = %this.getName();
  
  if (%name !$= "TimeTravelItem")
  {
   // Jeff: can't pick up more powerups if you have the helicopter.
   // wanna know why? Cuz Buzzmusic100 told me so.
   if ($BUZZ::foundHelicopter)
   {
   messageClient(%user.client, '', '\c0You can not pick up any other PowerUps at this time!');
   return;
   }

   // Jeff: if it is a helicopter, set the variable to true
   if (%this.getName() $= "HelicopterItem")
   $BUZZ::foundHelicopter = true;
 }

     Parent::onPickup(%this, %obj, %user, %amount);
  }

  function onMissionLoaded()
  {
     Parent::onMissionLoaded();

     // Jeff: initalize important variables
     $BUZZ::usingHelicopter = false;
     $BUZZ::foundHelicopter = false;
  }

  function onMissionReset()
  {
     Parent::onMissionReset();      

     // Jeff: reinitalize important variables
     $BUZZ::usingHelicopter = false;
     $BUZZ::foundHelicopter = false;

     LocalClientConnection.player.stopAudio(0);
  LocalClientConnection.player.setPowerUpId(0, true);
  }

  function Marble::onPowerUpUsed(%this)
  {
     if ($BUZZ::foundHelicopter)
  return;
     Parent::onPowerUpUsed(%this);
  }

  function GameConnection::createPlayer(%this, %spawnPoint)
  {
     Parent::createPlayer(%this, %spawnPoint);
     %this.player.getDataBlock().powerUpTime[5] = "99999999999999999999999999";
  }

  function disconnect(%force)
  {
     Parent::disconnect(%force);
     deactivatePackage(specialHelicopter);
  }
};

activatePackage(specialHelicopter);